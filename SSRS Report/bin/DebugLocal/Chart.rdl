    <DataSet Name="Chart">
      <Query>
        <DataSourceName>Chart</DataSourceName>
        <CommandText>SELECT u.FirstName, a.Id,a.Date, a.INTime, a.OutTime, a.TotalHours,u.UserName
FROM tblUser as u
INNER JOIN tblAttendance as a
ON u.Id=a.UserId </CommandText>
      </Query>
