  <DataSets>
    <DataSet Name="Attendance_DataSet">
      <Query>
        <DataSourceName>Attendance_DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserName">
            <Value>=Parameters!UserName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT u.FirstName, a.Id,a.Date, a.INTime, a.OutTime, a.TotalHours
FROM tblUser as u
INNER JOIN tblAttendance as a
ON u.Id=a.UserId where FirstName = @UserName and a.Date &gt;= @StartDate and a.Date &lt;= @EndDate;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
